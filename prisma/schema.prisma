generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employee {
  id String @id @default(uuid()) @db.Uuid
  name String @unique
  emal String @unique
  role  Role
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
enum Role {
  INTERN
  FULLTIME 
  PARTTIME
}